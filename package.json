{
  "name": "rss-power",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:prisma": "prisma studio --hostname 0.0.0.0",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/app/generated",
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "~4.3.0",
    "@radix-ui/react-dialog": "~1.0.0",
    "@radix-ui/react-dropdown-menu": "~1.0.0",
    "@radix-ui/react-tooltip": "~1.0.0",
    "@remix-run/express": "~1.7.0",
    "@remix-run/node": "~1.7.0",
    "@remix-run/react": "~1.7.0",
    "@remix-run/server-runtime": "~1.7.0",
    "@tailwindcss/forms": "~0.5.1",
    "@tailwindcss/line-clamp": "~0.4.0",
    "bcryptjs": "~2.4.3",
    "classnames": "~2.3.1",
    "compression": "~1.7.4",
    "cross-env": "~7.0.3",
    "date-fns": "~2.29.2",
    "express": "~4.18.1",
    "isbot": "~3.5.2",
    "isomorphic-dompurify": "~0.22.0",
    "metascraper": "~5.30.3",
    "metascraper-author": "~5.30.3",
    "metascraper-date": "~5.30.3",
    "metascraper-description": "~5.30.3",
    "metascraper-image": "~5.30.3",
    "metascraper-logo": "~5.30.3",
    "metascraper-publisher": "~5.30.3",
    "metascraper-title": "~5.30.3",
    "morgan": "~1.10.0",
    "phosphor-react": "~1.4.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "rss-parser": "~3.12.0",
    "tiny-invariant": "~1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@babel/preset-env": "7",
    "@faker-js/faker": "~7.5.0",
    "@remix-run/dev": "~1.7.0",
    "@remix-run/eslint-config": "~1.7.0",
    "@storybook/addon-actions": "~6.5.10",
    "@storybook/addon-essentials": "~6.5.10",
    "@storybook/addon-interactions": "~6.5.10",
    "@storybook/addon-links": "~6.5.10",
    "@storybook/addon-postcss": "~2.0.0",
    "@storybook/builder-vite": "~0.2.2",
    "@storybook/react": "~6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/cypress": "~8.0.2",
    "@testing-library/dom": "~8.17.0",
    "@testing-library/jest-dom": "~5.16.3",
    "@testing-library/react": "~13.3.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/bcryptjs": "~2.4.2",
    "@types/compression": "~1.7.2",
    "@types/eslint": "~8.4.1",
    "@types/express": "~4.17.13",
    "@types/morgan": "~1.9.3",
