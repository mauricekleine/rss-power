    "@types/react": "~18.0.18",
    "@types/react-dom": "~18.0.6",
    "@vitejs/plugin-react": "~2.0.1",
    "@vitest/coverage-c8": "~0.22.1",
    "autoprefixer": "~10.4.4",
    "babel-loader": "~8.2.5",
    "c8": "~7.12.0",
    "cypress": "~10.7.0",
    "dotenv": "~16.0.0",
    "esbuild": "~0.15.6",
    "eslint": "~8.23.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-storybook": "~0.6.4",
    "eslint-plugin-unused-imports": "~2.0.0",
    "happy-dom": "~6.0.4",
    "msw": "~0.46.1",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.4.12",
    "prettier": "~2.7.1",
    "prettier-plugin-tailwindcss": "~0.1.13",
    "prisma": "~4.3.0",
    "start-server-and-test": "~1.14.0",
    "tailwindcss": "~3.1.8",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.1.0",
    "type-fest": "~2.19.0",
    "typescript": "~4.8.2",
    "vite": "~3.0.9",
